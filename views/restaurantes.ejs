<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles/restaurantes.css">
    <link rel="shorcut icon" href="./imagenes/restaurante.png" type="image/x-icon">
    <title>Restaurantes</title>
</head>
<body>
    <!--INICIO-SECCION CARRITO DE COMPRAS OVERLAY====================================================================================-->



    <section class="carrito-compras">
        <div class="modal-container" id="modal_container">
            <div class="modal">
                <div class="car-x">
                    <h1 class="text-carrito">Carrito de Compras</h1>
                    <a id="close">
                        <img src="./imagenes/x-gris.png" alt=""></img>
                    </a>
                </div>
                <div class="barra-roja"></div>
                <div class="ordenes-comida">
                </div>

                <div class="confirmar-pedido">
                    <h4>Total del carrito</h4>
                    <div class="barra-roja"></div>
                    <div class="total-carrito">
                        <h1>Subtotal: </h2>
                            <h1 class="precio">$0</h1>
                    </div>
                    <button class="button-confirmar">Confirmar Pedido</button>
                </div>
            </div>
        </div>
    </section>

    <!--FINAL-SECCION CARRITO DE COMPRAS OVERLAY====================================================================================-->


    <!--INICIO-SECCION NAVBAR HEADER====================================================================================-->

    <header>
        <div class="container-inicial">
            <img class="icono-correo" src="./imagenes/correo-electronico.png" alt="icono-correo"></img>
            <h5 class="correo-info">info@ejemplo.com</h5>
            <img class="icono-face-head" src="./imagenes/facebook.png" alt=""></img>
            <img class="icono-twitter-head" src="./imagenes/twitter.png" alt=""></img>
            <img class="icono-insta-head" src="./imagenes/instagram.png" alt=""></img>
            <img class="icono-you-head" src="./imagenes/youtube.png" alt=""></img>
            <a href="/signin">
                <h5 class="login">Iniciar Sesión</h5>
            </a>
        </div>
        <div class="nav-bar">
            <a href="/" class="logo">
                <img src="./imagenes/Logo.png" alt="logoimg"></img>
            </a>
            <nav class="links">
                <a href="/" class="nav-link">Inicio</a>
                <a href="/Blogs" class="nav-link">Blogs</a>
                <a href="/Restaurantes" class="nav-link">Restaurantes</a>
                <a href="/registro" class="nav-link">Registrarse</a>
                <ul class="menu">
                    <li>
                        <a href="/Blogs" class="nav-link">
                            Paginas
                            <img class="flecha-abajo" src="./imagenes/flecha-hacia-abajo-para-navegar.png" alt=""></img>
                        </a>
                        <ul class="submenu">
                            <li><a href="">Contacto</a></li>
                            <li><a href="">Terminos y condiciones</a></li>
                        </ul>
                    </li>
                </ul>
                

                <button class="button-carrito" id="open">
                    <img class="icono-carrito" src="./imagenes/carrito-de-compras.png" alt=""></img>
                </button>

            </nav>
        </div>
    </header>

    <script src="/js/inicio.js"></script>

    <!--FINAL-SECCION NAVBAR HEADER====================================================================================-->

    <section class="containter">
        <div class="filtro">
            <div class="filtrar">
                <h1>Filtrar</h1>
                <div>
                    <h2>Cerca de mi</h2>
                </div>
                <div>
                    <h2>Domicilio</h2>
                </div>
                <div>
                    <h2>Lo mas elegido</h2>
                </div>
            </div>
            <div class="categorias">
                <h1>Catergorias</h1>
                <div>
                    <h2>Ensaladas</h2>
                </div>
            </div>
        </div>
        <div class="containerres">
            <div class="apartadores">
                <h1>Contacte gratis</h1>
                <h2>Tu domiciliario entregara tu orden a la puerta de tu hogar</h2>
            </div>
            <div class="wrap">
                <% results.forEach((user)=>{%>
                <div class="elementores">
                    <img class="imgele" src="/uploads/<%- user.icono_res %>" alt="">
                    <div class="infoelmento">
                        <div class="titlestar">
                            <% if(user.iduser_res == name){ %>
                                <a href="/Restaurantes/%20<%- user.idrestaurantes %>/%20<%- name %>"><h1><%- user.name_res %></h1></a>
                            <% }else{ %>
                                <a href="/RestauranteProfile/<%- user.idrestaurantes %>"><h1><%- user.name_res %></h1></a>
                            <% } %>
                            <div class="star-com">
                                <img src="./imagenes/estrella.png" alt=""></img>
                                <img src="./imagenes/estrella.png" alt=""></img>
                                <img src="./imagenes/estrella.png" alt=""></img>
                                <img src="./imagenes/estrella.png" alt=""></img>
                                <img src="./imagenes/estrellam.png" alt=""></img>
                            </div>
                        </div>
                        <h2>Tipo de comida: Hamburguesa, helado y papa fritas</h2>
                        <div class="horadirec">
                            <img class="iconores" src="./imagenes/fast-food-logo-design-free-vector.jpg" alt="">
                            <div class="conthoradirec">
                                <div>
                                    <img src="/imagenes/reloj-de-pared.png" alt="">
                                    <h2><%- user.hora_open %> - <%- user.hora_close %></h2>
                                </div>
                                <div>
                                    <img src="/imagenes/maparojo.png" alt="">
                                    <h2><%- user.direccion_res %></h2>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                </div>
                <%})%>
            </div>
        </div>
    </section>

     <!--INICIO-SECCION FOOTER==================================================================================-->

     <section class="final">
        <footer class="footer-inicial">
            <div class="bar-vacia"></div>
            <div class="footer-info">
                <div class="footer-info1">
                    <a href="/"><img src="./imagenes/Logo.png" alt=""></img></a>
                    <h6>Lorem ipsum dolor sit amet <br></br>consectetur adipiscing elit orci <br></br>porttitor, mattis
                        arcu.</h6>
                </div>
                <div class="footer-info2">
                    <h1>Enlace rápido</h1>
                    <a href="/">
                        <h6>Inicio</h6>
                    </a>
                    <a href="/Restaurantes">
                        <h6>Todos los restaurantes</h6>
                    </a>
                    <a href="">
                        <h6>Contacto</h6>
                    </a>
                    <a href="">
                        <h6>Detalles del Blog</h6>
                    </a>
                </div>
                <div class="footer-info3">
                    <h1>Enlace rápido 2</h1>
                    <a href="/Signin">
                        <h6>Inicio de sesión</h6>
                    </a>
                    <a href="/Registro">
                        <h6>Resgistrarse</h6>
                    </a>
                    <a href="">
                        <h6>Términos y condiciones</h6>
                    </a>
                </div>
                <div class="footer-info4">
                    <h1>Contacto</h1>
                    <div>
                        <img src="./imagenes/maparojo.png" alt=""></img>
                        <h6>Tr 39 No. 57A-14, C.P 11001</h6>
                    </div>
                    <div>
                        <img src="./imagenes/llamada-telefonica.png" alt=""></img>
                        <h6>+57 312 587 1547</h6>
                    </div>
                    <div>
                        <img src="./imagenes/correo.png" alt=""></img>
                        <h6>info@ejemplo.com</h6>
                    </div>
                </div>
            </div>

            <div class="bar-footerinfo">
                <div class="blogs-textcopy">
                    <h6 class="text-redcopy">Copyright © 2023 </h6>
                    <h6 class="text-redcopy2">Yum</h6>
                    <h6 class="text-redcopy3">. Todos los derechos reservados.</h6>
                </div>
                <div class="blogs-iconosfoot">
                    <a href=""><img class="icono-face-head" src="./imagenes/facebook.png" alt=""></img></a>
                    <a href=""><img class="icono-twitter-head" src="./imagenes/twitter.png" alt=""></img></a>
                    <a href=""><img class="icono-insta-head" src="./imagenes/instagram.png" alt=""></img></a>
                    <a href=""><img class="icono-you-head" src="./imagenes/youtube.png" alt=""></img></a>
                </div>
            </div>

        </footer>
    </section>
</body>
</html>